create table ON_DUTY_PERSONNEL
(
   id integer not null,
   name varchar(255) not null,
   deviceType varchar(255) not null,
   location varchar(255) not null,
   primary key(id)
);

create table ON_DUTY_PERSONNEL
(
   vehicleNumber varchar(255) not null,
   violationData varchar(255) not null,
   violationDate varchar(255) not null,
   penalty integer not null,
   location varchar(255) not null,
   primary key(id)
);

